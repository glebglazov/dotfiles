# Ignore known hosts
.ssh/known_hosts

# Ignore lazy-lock
.config/nvim/lazy-lock.json

# Ignore some oh-my-zsh folders
.oh-my-zsh/cache/**/*
.oh-my-zsh/templates/**
.oh-my-zsh/custom/themes/powerlevel10k/**/*.zwc

# Ignore machine-specific directories
{{ if not .work -}}
private/Dev/work
{{ end -}}

{{ if not .personal -}}
private/Dev/personal
{{ end -}}

{{ if not .mixed -}}
private/Dev/mixed
{{ end -}}
